{
  "name": "artisashop-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "postinstall": "patch-package",
    "api:fetch": "rimraf swagger.json && wget https://localhost:7095/swagger/v1/swagger.json",
    "api:generate": "rimraf ./src/api && openapi-generator-cli generate -i swagger.json -g typescript-fetch -o ./src/api"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.19.0",
    "@tanstack/react-router": "beta",
    "@types/node": "^18.11.10",
    "jotai": "^1.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "patch-package": "^6.5.0",
    "postcss": "^8.4.19",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-svgr": "^2.2.2"
  }
}
