                              },
                              "tranche_effectif_salarie": {
                                "type": "string",
                                "example": "41"
                              },
                              "date_debut_activite": {
                                "type": "string",
                                "format": "date",
                                "nullable": true,
                                "example": "2014-04-29"
                              },
                              "etat_adiministratif": {
                                "type": "string",
                                "example": "A"
                              },
                              "activite_principale": {
                                "type": "string",
                                "example": "53.10Z"
                              },
                              "complement_adresse": {
                                "type": "string",
                                "example": "DIRECTION GENERALE DE LA POSTE"
                              },
                              "numero_voie": {
                                "type": "string",
                                "example": "9"
                              },
                              "indice_repetition": {
                                "nullable": true,
                                "example": null
                              },
                              "type_voie": {
                                "type": "string",
                                "example": "RUE"
                              },
                              "libelle_voie": {
                                "type": "string",
                                "example": "DU COLONEL PIERRE AVIA"
                              },
                              "distribution_speciale": {
                                "type": "string",
                                "nullable": true,
