                                "nullable": true,
                                "example": null
                              },
                              "libelle_commune_etranger": {
                                "type": "string",
                                "nullable": true,
                                "example": null
                              },
                              "libelle_pays_etranger": {
                                "type": "string",
                                "nullable": true,
                                "example": null
                              },
                              "adresse_complete": {
                                "type": "string",
                                "example": "DIRECTION GENERALE DE LA POSTE 9 RUE DU COLONEL PIERRE AVIA 75115 PARIS 15"
                              },
                              "adresse_complete_secondaire": {
                                "type": "string",
                                "nullable": true,
                                "example": null
                              },
                              "code_postal": {
                                "type": "string",
                                "pattern": "^((0[1-9])|([1-8][0-9])|(9[0-8])|(2A)|(2B))[0-9]{3}$",
                                "example": "75015"
                              },
                              "departement": {
                                "type": "string",
                                "pattern": "\\b([013-8]\\d?|2[aAbB1-9]?|9[0-59]?|97[12346])\\b",
                                "example": "75"
                              },
                              "geo_id": {
                                "type": "string",
                                "example": "75115_2214"
                              },
                              "longitude": {
                                "type": "number",
                                "nullable": true,
                                "format": "float",
                                "example": 2.275688
                              },
                              "latitude": {
                                "type": "number",
                                "nullable": true,
                                "format": "float",
                                "example": 48.83002
                              },
                              "activite_principale_registre_metier": {
                                "type": "string",
                                "nullable": true,
                                "example": null
                              }
                            }
                          },
                          "date_creation": {
                            "type": "string",
                            "format": "date",
                            "nullable": true,
                            "example": "1991-01-01"
                          },
                          "tranche_effectif_salarie": {
                            "type": "string",
                            "example": "53"
                          },
                          "date_mise_a_jour": {
                            "type": "string",
                            "format": "date-time",
                            "nullable": true,
                            "example": "2022-05-31T08:00:12"
                          },
                          "categorie_entreprise": {
                            "type": "string",
                            "example": "GE"
                          },
                          "etat_administratif": {
                            "type": "string",
                            "example": "A"
                          },
                          "nom_raison_sociale": {
                            "type": "string",
                            "example": "LA POSTE"
                          },
                          "nature_juridique": {
                            "type": "string",
                            "example": "5510"
                          },
                          "activite_principale": {
                            "type": "string",
                            "example": "53.10Z"
                          },
                          "section_activite_principale": {
